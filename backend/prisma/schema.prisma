generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Cafes {
  id          Int    @id @default(autoincrement())
  name        String @unique
  description String
  logo        String
  location    String
  employees   Int
}

model Employees {
  id            Int    @id @default(autoincrement())
  name          String
  email_address String
  phone_number  String
  days_worked   Int
  cafe          String @unique
}
